{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [
            true, 120
        ],
        "quotemark": [
            true, "single"
        ],
        "no-console": [
            false
        ],
        "ban-types": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "no-namespace": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "interface-name": [
            false
        ],
        "object-literal-sort-keys": false,
        "trailing-comma": [
            false
        ]
    },
    "rulesDirectory": []
}
